        <doctype>
            <name>Blog main page</name>
            <type>page</type>
            <module>Blog</module>
            <controller>entry</controller>
            <action>default</action>
        </doctype>
        <doctype>
            <name>Blog latest entries</name>
            <type>snippet</type>
            <module>Blog</module>
            <controller>snippet</controller>
            <action>latest</action>
        </doctype>
        <doctype>
            <name>Blog categories</name>
            <type>snippet</type>
            <module>Blog</module>
            <controller>snippet</controller>
            <action>categories</action>
        </doctype>
        <doctype>
            <name>Blog calendar</name>
            <type>snippet</type>
            <module>Blog</module>
            <controller>snippet</controller>
            <action>calendar</action>
        </doctype>
        <doctype>
            <name>Blog feed</name>
            <type>snippet</type>
            <module>Blog</module>
            <controller>snippet</controller>
            <action>feed</action>
        </doctype>
    </doctypes>
</zend-config>
