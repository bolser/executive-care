<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <plugin>
        <pluginName>Blog</pluginName>
        <pluginDescription>
            Blog plugin based on pimcore objects with categories, tags and comments.
        </pluginDescription>
        <pluginNiceName>Blog</pluginNiceName>
        <pluginIcon>/plugins/Blog/static/img/.png</pluginIcon>
        <pluginVersion>1.0.5</pluginVersion>
        <pluginClassName>Blog_Plugin</pluginClassName>
        <pluginIncludePaths>
            <path>/Blog/lib</path>
            <path>/Blog/controllers</path>
        </pluginIncludePaths>
        <pluginNamespaces>
            <namespace>Modern</namespace>
        </pluginNamespaces>
    </plugin>
</zend-config>
